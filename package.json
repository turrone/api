{
  "name": "@turrone/turrone-server",
  "version": "0.1.0",
  "description": "A NuGet server written in Node. Designed to host a Chocolatey internal package feed.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.js": [
        "eslint --fix",
        "git add"
      ],
      "*.{json,md}": [
        "prettier --write",
        "git add"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/turrone/turrone-server.git"
  },
  "keywords": [
    "Turrone",
    "NuGet",
    "Chocolatey"
  ],
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/turrone/turrone-server/issues"
  },
  "homepage": "https://github.com/turrone/turrone-server#readme",
  "devDependencies": {
    "eslint": "^5.12.1",
    "eslint-config-prettier": "^3.6.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "prettier": "^1.16.0"
  },
  "dependencies": {
    "express": "^4.16.4"
  }
}
